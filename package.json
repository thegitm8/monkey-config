{
  "name": "monkey-config",
  "version": "0.4.0",
  "description": "An environment aware configuration build tool.",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "npm run clear && tsc",
    "build:watch": "tsc --watch",
    "preversion": "npm run clear && npm run build && npm run test",
    "postversion": "git push origin master --tags && npm publish",
    "test": "nyc -s mocha --recursive dist/test",
    "clear": "rimraf dist coverage .nyc_output"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thegitm8/monkey-config.git"
  },
  "keywords": [
    "config",
    "configuration",
    "environment variables",
    "twelve factor"
  ],
  "author": "Doc <mail@antlike.tech>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thegitm8/monkey-config/issues"
  },
  "homepage": "https://github.com/thegitm8/monkey-config#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.106",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.1",
    "chai": "^4.1.2",
    "git-branch-is": "^0.1.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "lodash": "^4.17.5"
  }
}
